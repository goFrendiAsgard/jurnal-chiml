out: message
do:
    - (Math.round(Math.random() * 100)) --> answer

    - do:
          - ("Enter a number: ") -> [sys.prompt] -> number
          - if: number < answer
            do: ("Too small") -> [sys.print]
          - if: number > answer
            do: ("Too big") -> [sys.print]
      while: number != answer

    - ("Thank you for playing") --> message
