out: genres
do:
  - |('http://localhost:3000/genres') -> [$.httpRequestBody] -> genres
  - map: genres
    into: genres
    ins: raw
    out: result
    do: 
      - |('http://localhost:3000/books?genreId=' + raw.id) -> [$.httpRequestBody] -> books
      - map: books
        into: books
        do: |(book) -> (x) => x.title -> book
      - |({genre: raw.name, books}) --> result
